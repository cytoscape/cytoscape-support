#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<!-- 
	This simply constructs an object (or bean) called "sampleAnalyzerImpl"
	of type ${package}.internal.SampleAnalyzerImpl.  The name "sampleTaskImpl"
	is used to identify this particular object in this and the 
	bundle-context-osgi.xml file. 
	-->
	<bean id="sampleAnalyzerImpl"
		class="${package}.internal.SampleAnalyzerImpl">
	</bean>

</beans>
